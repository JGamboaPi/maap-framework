      "require": "./build/cache/memory-cache.cjs"
    },
    "./cache/redis": {
      "types": "./build/cache/redis-cache.d.ts",
      "import": "./build/cache/redis-cache.js",
      "require": "./build/cache/redis-cache.cjs"
